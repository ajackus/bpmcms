{
  "kind": "collectionType",
  "collectionName": "registrations",
  "info": {
    "name": "Registration"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone_no": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "avatar_id": {
      "collection": "avatar",
      "via": "registration_id"
    },
    "session_id": {
      "model": "session"
    }
  }
}
